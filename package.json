{
  "name": "ahnenbaum",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=22"
  },
  "workspaces": [
    "packages/core",
    "packages/server",
    "packages/client"
  ],
  "scripts": {
    "build": "npm run build --workspaces",
